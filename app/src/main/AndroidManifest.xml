<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="liu.myapplication">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.AnimationActivity"></activity>
        <activity android:name=".ui.CustomViewActivity"></activity>
        <activity android:name=".ui.OkhttpActivity"></activity>
        <activity android:name=".ui.ObserverTestActivity"></activity>
        <activity
            android:theme="@style/transparentTitleTheme"
            android:name=".ui.HeaderScrollViewActivity"></activity>
        <activity
            android:launchMode="singleTop"
            android:name=".ui.RemoteViewActivity"></activity>
        <meta-data
            android:name="UMENG_CHANNEL"
            android:value="wandoujia" />
    </application>

</manifest>